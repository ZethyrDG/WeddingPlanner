@model Wedding
<h1>Plan Your Wedding</h1>
<a href="/dashboard">Dashboard</a>
<a href="/logout">Log Out</a>
<form asp-action="AddWedding" asp-controller="Home" method="post">
    <label asp-for="Wedder1Name"></label>
    <input asp-for="Wedder1Name">
    <span asp-validation-for="Wedder1Name"></span>
    <br>
    <label asp-for="Wedder2Name"></label>
    <input asp-for="Wedder2Name">
    <span asp-validation-for="Wedder2Name"></span>
    <br>
    <label asp-for="Date"></label>
    <input asp-for="Date">
    <span asp-validation-for="Date"></span>
    <br>
    <label asp-for="Address"></label>
    <input asp-for="Address">
    <span asp-validation-for="Address"></span>
    <br>
    <input type="submit" value="Create">
</form>